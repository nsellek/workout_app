<div class="mt-3 mx-3">
  <h3>Clients</h3>
  <div class="row mt-2 clients">
    <% @clients.each do |client| %>
      <%= render 'tile', client: client %>
    <% end %>

    <div class="col col-md-2">
      <div class="card p-2">
        <img src="<%= add_client_img %>" alt="" class="card-img-top">
        <div class="card-body text-center">
          <h5 class="card-title">Add New Client</h5>
        </div>
      </div>
    </div>
  </div>
</div>
